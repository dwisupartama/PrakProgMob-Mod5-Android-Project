<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ModulPenduduk.PegawaiEditPendudukActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="30dp"
            android:paddingBottom="30dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center|center_horizontal"
                android:layout_marginRight="10dp">
                <ImageButton
                    android:id="@+id/btn_back"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/bg_img_btn_ripple"
                    android:foreground="@drawable/ripple_img_button_ripple"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:src="@drawable/ic_arrow_left"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:alpha="0.75"
                    android:textSize="15dp"
                    android:text="Perbaharui Penduduk"
                    />
                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Perbaharui Penduduk"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/poppins_semibold"
                android:textSize="28dp"
                android:textColor="@color/PrimaryTextColorBlack"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Perbaharui penduduk ketika ada data yang  tidak sesuai atau permintaan penduduk"
                android:fontFamily="@font/poppins_regular"
                android:textSize="15dp"
                android:paddingRight="100dp"
                android:textColor="@color/PrimaryTextColorBlack"
                android:alpha="0.5"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>

            <LinearLayout
                android:id="@+id/ln_status_akses"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="-30dp"
                android:visibility="gone">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Status Akses"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <Spinner
                    android:id="@+id/sp_status_akses"
                    android:background="@drawable/bg_spinner_primary"
                    android:elevation="10dp"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:spinnerMode="dialog"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:entries="@array/list_status_akses"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false"
                android:layout_marginTop="30dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nomor Induk Kependudukan (NIK)"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <EditText
                    android:id="@+id/et_nik"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:hint="Masukkan Nomor Induk Kependudukan"
                    android:elevation="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/style_primary_input"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:textColorHint="@color/transparent50PrimaryText"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:inputType="text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Reset Password"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <Button
                    android:id="@+id/btn_reset_password"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="7dp"
                    android:layout_marginBottom="20dp"
                    style="@style/buttonPrimary"
                    android:text="Reset Password"
                    android:drawableLeft="@drawable/ic_ubah_password"
                    android:paddingTop="17dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="35dp"
                    android:paddingRight="40dp"
                    android:elevation="10dp"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nama Lengkap"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <EditText
                    android:id="@+id/et_nama_lengkap"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:hint="Masukkan Nama Lengkap"
                    android:elevation="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/style_primary_input"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:textColorHint="@color/transparent50PrimaryText"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:inputType="text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Tempat Lahir"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <EditText
                    android:id="@+id/et_tempat_lahir"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:hint="Masukkan Tempat Lahir"
                    android:elevation="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/style_primary_input"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:textColorHint="@color/transparent50PrimaryText"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:inputType="text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Tanggal Lahir"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="130dp"
                        android:layout_height="55dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="20dp"
                        android:layout_marginTop="7dp"
                        android:background="@drawable/bg_layout_input_readonly"
                        android:gravity="center">
                        <TextView
                            android:id="@+id/tv_tanggal_lahir"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:text="--/--/----"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:fontFamily="@font/poppins_medium"
                            android:textSize="15dp"
                            android:textAlignment="center"
                            android:textColor="@color/PrimaryTextColorBlack"/>
                    </LinearLayout>
                    <ImageButton
                        android:id="@+id/imb_tanggal"
                        android:layout_width="55dp"
                        android:layout_height="55dp"
                        android:src="@drawable/ic_calendar"
                        android:background="@drawable/bg_icon_circle_primary"
                        android:foreground="@drawable/ripple_img_button_ripple"
                        android:layout_marginTop="7dp"
                        android:elevation="10dp"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Jenis Kelamin"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <RadioGroup
                    android:id="@+id/group_jk"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/jk_lakilaki"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="Laki - Laki"
                        android:textColor="@drawable/style_text_grup_radio"/>
                    <RadioButton
                        android:id="@+id/jk_perempuan"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="Perempuan"
                        android:textColor="@drawable/style_text_grup_radio"/>
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Golongan Darah"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <RadioGroup
                    android:id="@+id/group_golongan_darah"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/golongan_darah_a"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="A"
                        android:textColor="@drawable/style_text_grup_radio"/>
                    <RadioButton
                        android:id="@+id/golongan_darah_b"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="B"
                        android:textColor="@drawable/style_text_grup_radio"/>
                    <RadioButton
                        android:id="@+id/golongan_darah_ab"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="AB"
                        android:textColor="@drawable/style_text_grup_radio"/>
                    <RadioButton
                        android:id="@+id/golongan_darah_o"
                        android:layout_width="wrap_content"
                        android:layout_height="55dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/bg_group_radio_button"
                        android:button="@drawable/null_icon_group_radio"
                        android:elevation="10dp"
                        android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                        android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                        android:fontFamily="@font/poppins_medium"
                        android:gravity="center"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp"
                        android:text="O"
                        android:textColor="@drawable/style_text_grup_radio"/>
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Alamat"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <EditText
                    android:id="@+id/et_alamat"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:hint="Masukkan Alamat"
                    android:elevation="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/style_primary_input"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:textColorHint="@color/transparent50PrimaryText"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:inputType="textMultiLine"
                    android:gravity="top"
                    android:paddingTop="15dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Agama"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <Spinner
                    android:id="@+id/sp_agama"
                    android:background="@drawable/bg_spinner_primary"
                    android:elevation="10dp"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:spinnerMode="dialog"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:entries="@array/list_agama"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Status Perkawinan"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <Spinner
                    android:id="@+id/sp_status_perkawinan"
                    android:background="@drawable/bg_spinner_primary"
                    android:elevation="10dp"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:spinnerMode="dialog"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:entries="@array/list_status_perkawinan"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:clipToPadding="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Pekerjaan"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"/>
                <EditText
                    android:id="@+id/et_pekerjaan"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:hint="Masukkan Pekerjaan"
                    android:elevation="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/style_primary_input"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textSize="15dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:textColorHint="@color/transparent50PrimaryText"
                    android:textColor="@color/PrimaryTextColorBlack"
                    android:inputType="text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_alignParentBottom="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="15dp"
                android:layout_marginTop="10dp"
                android:clipToPadding="false"
                android:gravity="center">

                <Button
                    android:id="@+id/btn_perbaharui"
                    style="@style/buttonPrimary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:elevation="10dp"
                    android:outlineAmbientShadowColor="@color/transparent50PrimaryText"
                    android:outlineSpotShadowColor="@color/transparent50PrimaryText"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:text="Perbaharui Penduduk"
                    app:icon="@drawable/ic_edit_white"
                    app:iconGravity="textStart"
                    app:iconPadding="15dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>